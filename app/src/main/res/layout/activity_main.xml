<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/mainback5"
    android:paddingBottom="16dp"
    android:paddingLeft="22dp"
    android:paddingRight="22dp"
    android:paddingTop="35dp"
    tools:context="com.example.bruno.jogo_galo.MainActivity">


    <Button
        android:id="@+id/pvp"
        android:layout_width="167.16sp"
        android:layout_height="60sp"
        android:onClick="goPvP"
        android:text=""
        android:background="@drawable/p1vsp2"
        android:textSize="20sp"
        android:layout_alignBottom="@+id/pve"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="92dp" />

    <Button
        android:id="@+id/pve"
        android:layout_width="167.16sp"
        android:layout_height="60sp"
        android:onClick="goPvE"
        android:text=""
        android:background="@drawable/p1vsai"
        android:textSize="20sp"
        android:layout_marginBottom="67dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/pvp" />

</RelativeLayout>
